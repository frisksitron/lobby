{
  "name": "lobby-app",
  "version": "1.0.0",
  "description": "An Electron application with Solid and TypeScript",
  "main": "./out/main/index.mjs",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "check": "biome check .",
    "fix": "biome check --write .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:instance1": "powershell -Command \"$env:ALLOW_MULTIPLE_INSTANCES='true'; $env:LOBBY_INSTANCE_ID='1'; npx electron-vite dev\"",
    "dev:instance2": "powershell -Command \"$env:ALLOW_MULTIPLE_INSTANCES='true'; $env:LOBBY_INSTANCE_ID='2'; npx electron-vite dev\"",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "3.0.2",
    "@electron-toolkit/utils": "4.0.0",
    "@sapphi-red/web-noise-suppressor": "0.3.5",
    "electron-store": "11.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.12",
    "@electron-toolkit/tsconfig": "2.0.0",
    "@solid-primitives/devices": "1.3.1",
    "@solidjs/router": "0.15.4",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "25.0.10",
    "autoprefixer": "10.4.23",
    "electron": "40.0.0",
    "electron-builder": "26.4.0",
    "electron-vite": "5.0.0",
    "postcss": "8.5.6",
    "solid-icons": "1.2.0",
    "solid-js": "1.9.11",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-solid": "2.11.10"
  }
}
